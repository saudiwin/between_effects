@Article{olken2007,
  Title                    = {Monitoring Corruption: Evidence from a Field Experiment in Indonesia},
  Author                   = {Benjamin Olken},
  Year                     = {2007},
  Number                   = {2},
  Pages                    = {200-249},
  Volume                   = {115},

  Annotation               = {\newline Olken does an incredibly impressive randomized study of corruption in road-building in Indonesia. He managed to get the Indonesian government to agree to allow auditing to be assigned to a treatment or control group of villages receiving block grants. He then independently verified the quality of the road with an engineering team. The measurements of the engineering team allowed him to calculate the percentage lost of money spent between the actual and reported expenditures, thus estimating net corruption. He found that audits had a greater effect than the other treatment, grassroots participation, although when he finessed the second treatment he was able to make grassroots participation matter for at least some kinds of road expenditures. Overall a great study, only concern would be the violation of SUTVA with audits: i.e., did other villages learn about the audits and also do corruption less? If so this would only weaken his effect, thus making his study stronger. \newline \newline What is the impact of the study? Is formal accountability always better than community development? Note that the study has never been replicated. Also, the treatment is auditing of probability one versus community monitoring that is much weaker (invitations to attend meeting). \newline},
  Journaltitle             = {Journal of Political Economy},
  Owner                    = {Bob},
  Timestamp                = {2014.09.18}
}

