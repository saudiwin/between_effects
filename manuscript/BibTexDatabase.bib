@Book{inglehart2005,
  Title                    = {Modernization, Cultural Change and Democracy: The Human Development Sequence},
  Author                   = {Ronald Inglehart and Christian Welzel},
  Publisher                = {New York: Cambridge University Press},
  Year                     = {2005},

  Abstract                 = {\newline Inglehart wrote this book because he needed to have measures of democracy which preceded the survey analysis. His former work was widely criticized for reverse causality. Inglehart wants to re-emphasize a trajectory to modernity via economic change expressing itself in culture. Culture is independent of economics and shows path-dependence. Thus, econ --> culture --> politics, but econ also directly affects politics so culture has an independent trajectory. Relationship is probabilistic without full convergence. \newline \newline On effective democracy -- note that FH scores include a measure of state capacity, thus multiplying the scores by World Bank corruption index is rather odd. Aggregation rule is obscure. There is something wrong as well with the argument on face: many countries democratized before they became post-industrial. What is the causal link btw. self-expressive values and democracy? You want to make your own choices, hence you need institutions to allow you to make your own choices. We have to believe that people made these demands on the system. That demand must create a supply of democracy. Unfortunately, we don't know if the people who led the change to democracy were the ones who also were pro-democratic in terms of culture. This is what Coppedge calls the ecological fallacy. Even if they do demand, why should demand create its own supply? \newline \newline Inglehart recognizes he has 3 threats to validity: 1) endogeneity of time (institutions-->values), 2) autocorrelation (past democracy predicts future democracy) and 3) spurious causation in terms of development (GDP). He uses Vanhanen's index to measure socioeconomic development (very idiosyncratic). He also uses different samples to make different statistical models which he then compares against each other. \newline},
  Owner                    = {Bob},
  Timestamp                = {2014.09.16}
}

@Article{INO:4308840,
  Title                    = {Diplomacy and domestic politics: the logic of two-level games},
  Author                   = {Putnam, Robert D.},
  Journal                  = {International Organization},
  Year                     = {1988},

  Month                    = {6},
  Pages                    = {427--460},
  Volume                   = {42},

  Abstract                 = {ABSTRACT Domestic politics and international relations are often inextricably entangled, but existing theories (particularly “state-centric” theories) do not adequately account for these linkages. When national leaders must win ratification (formal or informal) from their constituents for an international agreement, their negotiating behavior reflects the simultaneous imperatives of both a domestic political game and an international game. Using illustrations from Western economic summitry, the Panama Canal and Versailles Treaty negotiations, IMF stabilization programs, the European Community, and many other diplomatic contexts, this article offers a theory of ratification. It addresses the role of domestic preferences and coalitions, domestic political institutions and practices, the strategies and tactics of negotiators, uncertainty, the domestic reverberation of international pressures, and the interests of the chief negotiator. This theory of “two-level games” may also be applicable to many other political phenomena, such as dependency, legislative committees, and multiparty coalitions.},
  Doi                      = {10.1017/S0020818300027697},
  ISSN                     = {1531-5088},
  Issue                    = {03},
  Keywords                 = {Int'l Env and Transitions, International Relations, Alliance Behavior},
  Numpages                 = {34},
