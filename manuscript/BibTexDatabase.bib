@Article{de2010regime,
  Title                    = {Regime change and revolutionary entrepreneurs},
  Author                   = {De Mesquita, Ethan Bueno},
  Journal                  = {American Political Science Review},
  Year                     = {2010},
  Number                   = {3},
  Pages                    = {446--466},
  Volume                   = {104},

  Annotation               = {--in his model, the vanguard party uses violence to signal support among the population for regime change, although the vanguard party itself has no private information (446-47) --he includes structural factors re: Skocpol, although he allows these structural factors to sometimes cause revolution and sometimes not (447) he suggests structural factors are washed out in the data due to stochastic "history" and "culture", thus structural factors do matter even if you can't see the effect (447) he also argues that vanguard violence happens when the equilibrium is already set on participation, thus it may not be the determining factor (447) vanguards try to influence the cut-off point at which participation can happen, i.e. more participation even if everyone coordinates on participation already (448) simple game with a threshold above which participation will cause regime change. each person has a common signal plus an idiosyncratic error term (449). because the vanguard violence function includes the private signal, a higher level of violence can imply that there is more anti-government sentiment than in fact exists, thus pushing forward more turnout than the equilibrium would otherwise have (454) --The only time that the vanguard has an effect on sparking revolutionary turnout is when the population coordinates on a low turnout equilibrium (but not zero) and when violence exceeds expectations. Otherwise the effect won't be seen at all, or will merely increase already high turnout. (459)},
  Keywords                 = {Social Movement Theory, Democratic Transitions},
  Owner                    = {Bob},
  Publisher                = {Cambridge Univ Press},
  Review                   = {--he refers to Kuran's analysis as "focal point" models (447-48), which global games challenge. particularly Morris and Shin (2004). in these games, people only participate if their private information is high enough. --he differs from Lohman because the extremists have more than one tool, i.e., participation. they can also engage in an attack, so it is not just about protests (448) --fundamentally, his model shows how revolutionary violence can affect the "cut-off point" that most individuals believe is necessary for a revolution to be successful (454) --he reverses the causality so that vanguards only matter when the distribution of preferences in the population is already supportive of revolution, so the violence is kind of a fait accompli},
  Timestamp                = {2014.01.28}
}

