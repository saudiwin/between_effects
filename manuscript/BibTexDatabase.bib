@Article{gerber2000,
  Title                    = {The Effects of Canvassing, Telephone Calls, and Direct Mail on Voter Turnout: A Field Experiment},
  Author                   = {Alan S. Gerber and Donald P. Green},
  Year                     = {2000},
  Number                   = {3},
  Pages                    = {653-663},
  Volume                   = {94},

  Annotation               = {\newline Gerber and Green did a massive randomized study in New Haven of voting turnout behavior. They used mailings, telephone calls and personal visits as treatments, dividing a randomized sample into multiple treatment groups so they could look at interaction effects. They had a very low treatment to intent-to-treatment rate; they argued they could adjust for this by using the rate of later voter turnout among the control and treatment groups as a control for any potential confounders. They also used an IV method based on the same logic -- they selected the members of the treatment group randomly, so they could use the assignment to treatment as an IV for treatment itself. It is a clever idea, although I am not yet sure or convinced of the underlying statistical methodology. \newline \newline Class discussion: They have a very complicated treatments, with multiple conditions re: canvassing, mailings and telephone calls. This means 45 different kinds of treatment according to critics. Their randomization requires them to use multivariate analysis (there is induced correlation in the treatments... or was it error?). These kinds of experiments require a lot of thought and effort. This is an omnibus design that tries to do way too much. \newline \newline They only use the IV on the cavassing. Wouldn't you also need it for direct mail? They don't know whether or not the mail is read in fact. Should they also focus on the ITT for the mail? Or should they look for the CACE? Also, are there multiplicative effects of treatments on each other? They should estimate the turnout for each combination of cells. \newline \newline Re: telephone calls, do we know at all what the message of the telephone call is? That seems to be a missing component of the study. \newline},
  Journaltitle             = {American Political Science Review},
  Keywords                 = {Experiments, Causal Methodology},
  Owner                    = {Robert Kubinec},
  Timestamp                = {2014.09.15}
}

@Article{gerring2007,
  Title                    = {An Experimental Template for Case Study Research},
  Author                   = {John Gerring and R McDermott},
  Year                     = {2007},
  Number                   = {51},
  Pages                    = {688-701},

  Annotation               = {\newline Dynamic comparison -- two identical countries, one has a treatment over time and the other has no treatment over time. Both temporal and spatial variation. Longitudinal exploit only variation in time on a single case (turn on and off the light switch and assume no differences in terms of time changing the country). Spatial is simply the Method of Difference. Dynamic is clearly the best, then longitudinal and then spatial variation. But what is the basis for putting longitudinal above spatial? Longitudinal becomes more compelling as the amount of time decreases. \newline \newline Is it worthwhile using an experimental framework? Or are they trying to assume away the problem by arguing for exogeneity? If we want to use experiment as a template, then it is mostly relevant to making causal arguments. But if we do not want to make causal arguments, then experimental templates are not necessary. \newline},
  Journaltitle             = {American Journal of Political Science},
  Keywords                 = {Experiments, Causal Methodology, Qualitative},
  Owner                    = {Robert Kubinec},
