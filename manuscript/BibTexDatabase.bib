@Article{whitt2007,
  Title                    = {The Dictator Game, Fairness and Ethnicity in Postwar Bosnia},
  Author                   = {Sam Whitt and Rick K. Wilson},
  Year                     = {2007},

  Month                    = {July},
  Number                   = {3},
  Pages                    = {655-668},
  Volume                   = {51},

  Annotation               = {\newline Whitt and Wilson use the dictator game in post-conflict Bosnia. They have an impressive recruitment effort, involving nearly two hundred people of each of three ethnicities (Croat, Serb, Bosnian) from a random stratified sample (admittely, the sample had some issues, but response rate was very high). They do not use the Ultimatum or any other kind of game. They run two games, one in which people propose to their own ethnics, and a second in which they propose to other ethnicities. They then look at difference in means. They find that there is a difference, although it is marginally negative, and the median is zero (i.e., not a very strong effect, even if it passes statistical significance). In addition, overal levels of "fairness" measured by group offers are not different from other, less war-torn societies. \newline \newline They also estimate a multivariate model where they use ethnic threat perception and ties to see if it explains variance in in group versus out group offers. They find that there is a strong relationship between these variables, which questions their assertion that ethnicity was not a causal driving factor in the war. If strength of ties does in fact lower out group offers, then perhaps what has changed in the intervening years is the strength of these ties, not some overriding norm of fairness per se. \newline},
  Journaltitle             = {American Journal of Political Science},
  Keywords                 = {Causal Methodology, Experiments},
  Owner                    = {Bob},
  Timestamp                = {2014.09.10}
}

