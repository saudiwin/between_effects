@Article{Capoccia01082010,
  Title                    = {The Historical Turn in Democratization Studies: A New Research Agenda for Europe and Beyond},
  Author                   = {Capoccia, Giovanni and Ziblatt, Daniel},
  Journal                  = {Comparative Political Studies},
  Year                     = {2010},
  Number                   = {8-9},
  Pages                    = {931-968},
  Volume                   = {43},

  Abstract                 = {This article lays the theoretical and methodological foundations of a new historically minded approach to the comparative study of democratization, centered on the analysis of the creation, development, and interaction of democratic institutions. Historically, democracy did not emerge as a singular coherent whole but rather as a set of different institutions, which resulted from conflicts across multiple lines of social and political cleavage that took place at different moments in time. The theoretical advantage of this approach is illustrated by highlighting the range of new variables that come into focus in explaining democracy’s emergence. Rather than class being the single variable that explains how and why democracy came about, scholars can see how religious conflict, ethnic cleavages, and the diffusion of ideas played a much greater role in Europe’s democratization than has typically been appreciated. Above all, the authors argue that political parties were decisive players in how and why democracy emerged in Europe and should be at the center of future analyses.},
  Annotation               = {\newline Cappocia and Ziblatt make an argument against 1) cross-national studies of democratization using Boix's 0 or 1 approach to transitions and 2) studies that focus on "contemporaneous correlates" of democracy. Instead, building on Pierson, they argue for a study of the sequencing of historical causes, some of which may be "conjuctural", or occuring jointly with other causes. \newline In particular, they argue that socioeconomic development is not as decisive as once claimed, nor do neo-class theories of Boix and A&R do much better. In terms of Europe, the note that political coalitions often extended across class boundaries. Furthermore, they argue that parties are not merely carriers of class interests, but actors in their own right. In particular, they think party ideology is an influential variable in whether or not democracy is adopted. \newline They want to move away from a transitions paradigm to a view of democracy that is built one institution at a time (i.e. institutional complementarity, re: Thelen). Thus they think that democracy can be seen in terms of episodes in Europe, but that these episodes are defined by battles over particular institutional forms. Scholarship will advance as the collective causes of these institutional forms (and the sequencing) is made clearer. They also want to emphasize cases when institutional reform could have happend, but didn't (again echoing Mahoney \& Thelen). \newline I think this work is particularly illuminating for the Middle East. In particular, I wonder if the Middle East could be conceived of as three distinct critical junctures, from the post-WWI, post-WWII, and now the Arab Spring. Looking at the collective ideologies, economic interests and party formation in each period could provide key insight into both how and why democratization (or de-democratization) occurred in the Middle East. Perhaps a better title would simply be "regime transitions in the Middle East." \newline I could use some more work reviewing critical antecedents, Slater \& Simmons, and others. I think I understand it better now as an idea.},
  Eprint                   = {http://cps.sagepub.com/content/43/8-9/931.full.pdf+html},
  Owner                    = {Robert Kubinec},
  Timestamp                = {2014.07.10}
}

@Book{capoccia2005defending,
  Title                    = {Defending democracy: reactions to extremism in interwar Europe},
  Author                   = {Capoccia, Giovanni},
  Publisher                = {John Hopkins University Press},
  Year                     = {2005},

  Annotation               = {\newline Capoccia constructs a theory focusing on the defense of democracy from "anti-system" parties. It is heavily based on Sartori's work on parties which polarize democratic systems and force "border parties", or those who are closest to the extremist party, to either leave their coalition and join the extremists or risk a loss of votes. Capoccia examines this trade-off in Finland, Belgium, and Czechoslovakia during the inter-war period. These are cases of "democratic survival", as opposed to breakdown, which occured in Italy and Germany. In particular, he pinpoints the actions of heads of state who were willing to take on the extremist parties with both accomodation and repression--although repression was certainly required for survival. In order for the heads of state to have enough support, the border parties needed to remain in the centrist coalition. Thus he thinks of these moments as critical junctures in which heads of state needed to recognize the seriousness of the extremist problem (instead of just trying to moderate it through institutionalization) and border parties needed to accept a loss of votes in exchange for defending the system. \newline \newline Capoccia believes that this focus on actors complements structuralist theories which can't account for the outcome of these cases (e.g., Moore, Rueschemeyer et al). Critical junctures are when structures cease to have their controlling power. He wants more research done on structured counter-factual analysis exploring times when change occurred and when "re-equilibriation" happened (breakdown averted through the foresight of elites). \newline \newline He does not have a good criterion for determining when and how elites identify the seriousness of the extremist problem. He notes that the extremist parties camouflaged their activities in language supporting democracy. Thus it is not clear why the head of state in Czech was so much more willing to invest his political capital in fighting German extremists than the king of Italy was in fighting the fascists. Critical junctures exist, but what are the factors that influence the difference counter-factuals? That is the essence of manipulationist analysis--knowing what was the hammer which hit the window. The work could be much more structured so as to illumine the specific factors leading the actors to go in the ways in which they did. \newline},
  Keywords                 = {Democratic Transitions, Causal Methodology, Qualitative Methods},
  Owner                    = {Bob},
  Timestamp                = {2014.04.02}
}

