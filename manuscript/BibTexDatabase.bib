@Article{banerjee2005,
  Title                    = {History, Institutions and Economic Performance: The Legacy of Colonial Land Tenure Systems in India},
  Author                   = {Abhijit Banerjee and Lakshmi Iver},
  Year                     = {2005},

  Month                    = {September},
  Number                   = {4},
  Pages                    = {1190-1213},
  Volume                   = {95},

  Annotation               = {\newline Banerjee and Iver make a double-IV case for the effect of institutions in later Indian economic growth. Their instrument is the proportion of landlords versus non-landlords in India during British colonization. The argument is kind of convoluted, but the essential idea is that the presence of landlords at the point of colonization later affected public goods provision post-independence, thus leading to poorer agricultural outcomes. It is not clear, though, if the presence or absence of landlords affected economic development in any way other than via this effect on public goods provision, so they also instrument landlords by a binary variable for the period of British colonization. Thus there is a 2-stage estimation going on, with intermediate stages tested later in the argument (particularly landlords and public good provision). So the causal argument is that districts which were treated by British colonial officials to landlords versus districts that were treated by British colonial officials to non-landlords. This would be a treatment effect for the whole population of districts in India assuming that all districts were colonized and all of them are in the sample. \newline \newline Note that they don't just use IV, they also use a matching technique in terms of adjacent landlord-non landlord districts. How could British ideas be endogenous to landlord systems? There is a measurement problem in terms of district size. Boundaries can often change and you can't assume that all units stay the same over time. \newline},
  Journaltitle             = {The American Economic Review},
  Keywords                 = {Institutional Theory, Instrumental Variables, CPE},
  Owner                    = {Bob},
  Timestamp                = {2014.10.13}
}

