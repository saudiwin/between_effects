@Article{boix2011,
  Title                    = {Democracy, Development, and the International System},
  Author                   = {Carles Boix},
  Year                     = {2011},
  Number                   = {4},
  Pages                    = {809-828},
  Volume                   = {105},

  Annotation               = {\newline Boix puts forward a complementary argument to his assertion that both income and inequality matter for democratic transitions. In particular, he proposes that the international order mitigates and shapes the way that domestic variables influence transitions. He thinks that income matters mostly at low levels of income due to the declining marginal effect of income, while inequality matters at all income leves. However, income as a variable is most significant on democratic transition \& consolidation when the international order is receptive to democracy: i.e., when hegemons either don't have to worry about challengers with different institituions or a multi-polar world with institutional diversity across alliances. He finds that income is significant as an influence on democratization when the international order is most receptive, such as between WWI and WWII. \newline \newline He puts forward a well-developed database of his own Boix-Rosato codings and also the Polity database as a DV. He uses a variety of models with various attempts to instrument for income (such as genetic differences of populations in 1800). He also uses Granger causality tests and the full range of time series models to put forward his conclusions. Overall a strong argument, although the use of interactive variables is primarily restricted to categories even when he has continuous variables (i.e., log GDP). It seems to be a better strategy to use interactive graphs to find the effect. \newline},
  Journaltitle             = {American Political Science Review},
  Keywords                 = {Democratic Transitions, Inequality and Transitions, Int'l Env and Transitions},
  Owner                    = {Robert Kubinec},
  Timestamp                = {2014.10.01}
}

