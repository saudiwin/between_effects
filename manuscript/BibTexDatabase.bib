@Article{GOOP:GOOP134,
  Title                    = {Institutions Matter?},
  Author                   = {Przeworski, Adam},
  Journal                  = {Government and Opposition},
  Year                     = {2004},
  Number                   = {4},
  Pages                    = {527--540},
  Volume                   = {39},

  Annotation               = {Przeworski is very skeptical of attempts to distinguish the exogenous (environmental) and endogenous (recurring feedback loops) effect of institutional causation. He thinks there are too many possible confounders to state with any certainty what the relative causal weight of institutions is. Thus, nation-building projects are doomed. \newline The problem can be described in terms of what is exogenous, such as extant economic conditions, or endogenous, or the differing effect that environmental conditions have on institutions. (Admittedly, both of these definitions are somewhat vague). He argues that these two problems lead to several confounders of casual inference, including the difficulty of constructing valid counterfactuals that can allay the issues of "treated effect on the treated". In other words, we can't compare democracy v. non-democracy for the same country, so we only know the treated effect on the treated instead of the aggregate treatment effect. \newline However, does his analysis tend to be too pessimistic regarding institutions? It comes back to a fundamental nature of causality and the "order of things". If we expect our knowledge to complete, thorough, and mechanical, then we may never reach that goal. There are too many underlying disagreements about what the fundamental order of things is, even if we did want to go in that direction. Rather, we seem limited to provisional association, a consignment of medium-term causes to medium-term consequences. That may be as far as social science is able and willing to go.},
  Doi                      = {10.1111/j.1477-7053.2004.00134.x},
  ISSN                     = {1477-7053},
  Owner                    = {Robert Kubinec},
  Publisher                = {Blackwell Publishing Ltd.},
  Timestamp                = {2014.06.25},
  Url                      = {http://dx.doi.org/10.1111/j.1477-7053.2004.00134.x}
}

@Article{granovetter1978threshold,
  Title                    = {Threshold models of collective behavior},
  Author                   = {Granovetter, Mark},
  Journal                  = {American journal of sociology},
  Year                     = {1978},
  Pages                    = {1420--1443},

  Keywords                 = {Social Movement Theory},
  Owner                    = {Bob},
  Publisher                = {JSTOR},
  Review                   = {Granovetter creates the idea of a threshold, which is a composite of both the actor's preferences over outcomes and other actor-specific features, which could include risk preferences or even the context of the sitiuation. His model shows how different distributions of thresholds can yield very different collective action movements. Thresholds capture behavior that is influenced by the behavior of others. It may not apply equally to all actors. Hence: "Where the threshold effect is of little interest, and may be said to have been, in effect, falsified, is where all or most of the thresholds are at 0\% or 100\%--that is, where most people's behavior is not, in fact, contingent on that of the others" (1437)},
  Timestamp                = {2014.02.01}
}

@InBook{green2003,
  Title                    = {Reclaiming the Experimental Tradition in Political Science},
  Author                   = {Donald P. Green and Alan S. Gerber},
  Editor                   = {Ira Katznelson and Helen V. Milner},
  Pages                    = {805-833},
  Year                     = {2003},

  Annotation               = {\newline Green and Gerber make their best case for experimental field research. They claim that many trivial results have been produced in political science because no one has bothered to test these with field research. They call for more funding to be directed at field research instead of surveys. \newline \newline In answer to ethical questions, they argue that we need to consider both the costs and benefits of any experimental intervention, and that the costs are often morally ambiguous (admittedly this part of their paper needs more work). They want people to start small and then work big. In response to external validity challenges, they propose re-doing experiments in different locales, with a nod towards changing publishing rules in order to make this happen. \newline \newline Class discussion: depends on subfield. How would you do one with conflict studies? Works well in development econ and comparative/american politics. Gerber and Green have very much influenced AP. Field experiments are limited in their scope. Their answer to every critique is "do more experiments"... but is that always reasonable? They also downplay theories and their importance in research. But who is the audience then if it is not based on theory? \newline \newline The idea of experimental iteration: are observational studies more important? They are very anti-observational studies and think poli sci hasn't produced much new knowledge. If you do want to do this work, look for institutional opportunities. Especially with aid agencies and international organizations, although you have to cut through the red tape. \newline \newline Ethical concerns: be as minimally intrusive as you can. Is that a criterion? Supposedly these decisions will be made in the political process. But what about minimizing turnout? Would that be a "good" experiment? What are the substantial risks of harm? What is the value to society of your experiment?},
  Booktitle                = {Political Science: The State of the Discipline},
  Owner                    = {Robert Kubinec},
  Timestamp                = {2014.09.18}
}

