@Article{acemoglu2009,
  Title                    = {Reevaluating the Modernization Hypothesis},
  Author                   = {Daron Acemo\u{g}lu and James A Robinson and Simon Johnson and Pierre Yared},
  Year                     = {2009},
  Number                   = {8},
  Pages                    = {1043-1058},
  Volume                   = {56},

  Annotation               = {\newline Acemoglu et al put forward their own argument about income and democratization. They think that income does matter to democratization but that the channel is through the institutional makeup of countries. For that reason they re-estimate Przeworski and other models using country fixed effects. When they do so, the variation from income on the DV is no longer explained (they use Polity and Przeworksi as DVs). They think that the ``critical juncture" of colonization (re: their earlier work) explains these different institutional trajectories, although they are not clear why these trajectories would necessarily result in democratization. It appears that they are explaining differences in income more than differences in democratization. They also note that they are controlling for time covariation, which they assert soaks up a lot of variation and should be further explained. Note as well that Boix critiques them for using historical time windows that are too long and inflate standard errors (25-year lags). \newline},
  Journaltitle             = {Journal of Monetary Economics},
  Keywords                 = {Democratic Transitions, Inequality and Transitions},
  Owner                    = {Robert Kubinec},
  Timestamp                = {2014.10.01}
}

