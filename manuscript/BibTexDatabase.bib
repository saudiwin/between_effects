@Article{geddes2014,
  Title                    = {Autocratic Breakdown and Regime Transitions: A New Data Set},
  Author                   = {Barbara Geddes and Joseph Wright and Erica Frantz},
  Year                     = {2014},
  Number                   = {2},
  Pages                    = {313-331},
  Volume                   = {12},

  Annotation               = {\newline Geddes et al offer a new dataset specifically aimed at adjudicating between types of authoritarianism. It builds on Geddes (1999) and her work on transitions from different types of authoritarian regimes to democracy. What they argue makes their database distinct is the fact that current datasets, such as CGV, code dictatorship as a residual category. Hence, dictatorship longevity is overestimated because transitions from one authoritarian regime (i.e. civilian to military) will not show up in other datasets. \newline \newline They also argue their coding of different types of authoritarian regimes is more nuanced and detailed than other datasets. For example, they argue that CGV only uses whether a dictator belongs to the military as a marker for military regimes, whereas they look for the existence of a junta, a collegial governing body. However, like CGV, they also have a residual category, which for them is civilian dictatorships. They note there are a broad variety of forms of civilian dicatorships, and so they leave that as somewhat general (although they do distinguish between single party and personalist regimes). \newline \newline Finally, their preliminary conclusions based on their data show that transitions to democracy are more likely when there is less violence (they code how many deaths there are in any transition) and also when transitions happen from military regimes. They argue this is because militaries are better able to shield their leaders from later harm post-transition, whereas personalist regimes are the least likely to do so, hence democratization is less likely (and violent transitions are more likely). \newline},
  Date                     = {June},
  Journaltitle             = {Perspectives on Politics},
  Keywords                 = {Democratic Transitions, Causal Methodology},
  Owner                    = {Robert Kubinec},
  Timestamp                = {2014.09.04}
}

