@Article{habyarimana2007,
  Title                    = {Why Does Ethnic Diversity Undermine Public Goods Provision?},
  Author                   = {James Habyarimana and Macartan Humphreys and Daniel N. Posner and Jeremy M. Weinstein},
  Year                     = {2007},

  Month                    = {November},
  Number                   = {4},
  Pages                    = {709-725},
  Volume                   = {101},

  Annotation               = {\newline Habyarminana et al. provide a compelling series of experiments in Uganda focused on ethnicity and public good provision. They use a random sample of residents from urban slums which they pre-identified as ethnically diverse. They used large samples (100s of people) and ran them through several games. They were particularly intersted in testing not whether ethnicity has an effect on public goods provision, but how. To that end they postulated different mechanisms, including whether it is due to "strategic selection" (i.e. punishment in repeated play) or preferences (tastes for or against co-ethnics) or technology (ethnicity as means of reaching people "findability" or as a means to solving ends). They designed experiments which tested each of these hypotheses in turn, using a sophisticated strategy which allowed for both anonymous and non-anonymous play. Most of the participants did not know each other. \newline \newline They found that the strategic selection mechanism was the one in which ethnicity had its greatest effect on distinguishing play in both a Dictator game and in a PD game. They divided their sample into "egoists" and "non-egoists" based on observation of a simple dictator game, and they found that egoists were the ones who responded to the threat of sanction when play was nonanonymous. When anonymity was involved, egoists did not care about ethnicity. They also found that findability was an important part of ethnicity in a fascinating experiment where they had subjects try to locate random people in the village. Shared ethnicity made it much more likely they would be found. \newline},
  Journaltitle             = {American Political Science Review},
  Keywords                 = {Causal Methodology, Experiments, Ethnicity},
  Owner                    = {Bob},
  Timestamp                = {2014.09.10}
}

@Article{Haggard2012,
