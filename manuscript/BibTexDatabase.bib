@Article{PSR:5004264,
  Title                    = {Shaping Democratic Practice and the Causes of Electoral Fraud: The Case of Nineteenth-Century Germany},
  Author                   = {Ziblatt, Daniel},
  Journal                  = {American Political Science Review},
  Year                     = {2009},

  Month                    = {2},
  Pages                    = {1--21},
  Volume                   = {103},

  Abstract                 = { ABSTRACT Why is there so much alleged electoral fraud in new democracies? Most scholarship focuses on the proximate cause of electoral competition. This article proposes a different answer by constructing and analyzing an original data set drawn from the German parliament's own voluminous record of election disputes for every parliamentary election in the life of Imperial Germany (1871–1912) after its adoption of universal male suffrage in 1871. The article analyzes the election of over 5,000 parliamentary seats to identify where and why elections were disputed as a result of “election misconduct.” The empirical analysis demonstrates that electoral fraud's incidence is significantly related to a society's level of inequality in landholding, a major source of wealth, power, and prestige in this period. After weighing the importance of two different causal mechanisms, the article concludes that socioeconomic inequality, by making elections endogenous to preexisting social power, can be a major and underappreciated barrier to the long-term process of democratization even after the “choice” of formally democratic rules. },
  Annotation               = {\newline Ziblatt does an ingenuous intra-district study of 19th century Germany to test his thesis that landholding inequality blocked the institutionalization of democracy. While he relies on Boix, Robinson and Acemoglu to support his research question, he departs from Boix's insistence (and Przeworksi and Limongi) from differentiating between transitions to democracy and democratic consolidation. Instead, he argues for a gradualist perspective on democracy, in which power imbalances can invalidate democratic institutions even if these institutions have some power. \newline They use data from a German census of farms to construct a Gini coefficient for 19th century Germany. They also have reports from a marvelously efficient complaint resolution body that investigated complaints about electoral fraud. Combining these data, they show that districts in which electoral complaints were the highest were also the ones with the highest level of landholding inequality. \newline Their study shows the power of within-case variation. First, they are able to exploit much more detailed data than relying on national averages. Second, their analysis is able to hold the effects of German institutions and elections constant by virtue of using the same country. It seems that within-case variation could well be the wave of the future. \newline My only question about this piece is his argument for reconceptualizing democracy as both transition and consolidation. I understand the de jure/de facto power difference, but I still think that democratic transitions do exist and do matter. Even if democracy doesn't work as well as it should (on paper).},
  Doi                      = {10.1017/S0003055409090042},
  ISSN                     = {1537-5943},
  Issue                    = {01},
  Numpages                 = {21},
  Owner                    = {Robert Kubinec},
  Timestamp                = {2014.07.10},
  Url                      = {http://journals.cambridge.org/article_S0003055409090042}
}

@Article{PSR:5832060,
  Title                    = {From Violence to Voting: War and Political Participation in Uganda},
  Author                   = {Christopher Blattman},
  Journal                  = {American Political Science Review},
  Year                     = {2009},

  Month                    = {5},
  Pages                    = {231--247},
  Volume                   = {103},

  Abstract                 = {ABSTRACT What is the political legacy of violent conflict? I present evidence for a link from past violence to increased political engagement among excombatants. The evidence comes from northern Uganda, where rebel recruitment generated quasiexperimental variation in who was conscripted by abduction. Survey data suggest that abduction leads to substantial increases in voting and community leadership, largely due to elevated levels of violence witnessed. Meanwhile, abduction and violence do not appear to affect nonpolitical participation. These patterns are not easily explained by conventional theories of participation, including mobilization by elites, differential costs, and altruistic preferences. Qualitative interviews suggest that violence may lead to personal growth and political activation, a possibility supported by psychological research on the positive effects of traumatic events. Although the generalizability of these results requires more evidence to judge, the findings challenge our understanding of political behavior and point to important new avenues of research.},
  Doi                      = {10.1017/S0003055409090212},
  ISSN                     = {1537-5943},
  Issue                    = {02},
  Keywords                 = {Civil War, Democratic Transitions, Democracy and Civil Society},
  Numpages                 = {17},
