@Article{770442119550201,
  Title                    = {A Behavioral Model Of Rational Choice},
  Author                   = {Simon, Herbert A.},
  Journal                  = {Quarterly Journal of Economics},
  Year                     = {1955},
  Number                   = {1},
  Pages                    = {99 - 118},
  Volume                   = {69},

  Abstract                 = {The article presents definitions of rational choice that are modeled closely upon the actual decision processes pertaining to the behavior of organisms. Traditional economic theory postulates an "economic man," who, in the course of being "economic" is also "rational." This man is assumed to have knowledge of the relevant aspects of his environment which, if not absolutely complete, is at least impressively clear and voluminous. Recent developments in economics, and particularly in the theory of the business firm, have raised great doubts as to whether this schematized model of economic man provides a suitable foundation on which to erect a theory -- whether it be a theory of how firms do behave, or of how they "should" rationally behave. Broadly stated, the task is to replace the global rationality of economic man with a kind of rational behavior that is compatible with the access to information and the computational capacities that are actually possessed by organisms, including man},
  Annotation               = {\newline Simon lays out his original critique of rationalist assumptions, although he does so in a way that is meant to encourage rational choice explanation, not replace it. He has two primary objections: 1) the cognitive capacities of organisms may not measure up to what the model requires, and 2) the organism will have to gather information to construct a preference ranking and outcome mapping, which has a cost of its own. \newline His solution is to use heuristics, rules of thumb that allow actors to update their assessments of probabilities and preferences as they go along. Hence, a chess master will divide moves into winning or non-winning moves, and he will add to either set as he goes through the game in order to avoid the overwhelming cognitive demands of considering "all alternatives". It is clear how these ideas were later developed into more broad psychological theories. \newline In terms of economics, he thinks that firms likely hit generally accepted or unacceptable levels (a 0, 1 or -1) coding instead of a truly continuous function which would admit of maximization. People never think of, nor do they reach, maximums, but rather judge themselves relative to whatever their "aspiration level" is.},
  ISSN                     = {00335533},
  Keywords                 = {RATIONAL choice theory, ECONOMICS, DECISION making, BUSINESS enterprises, ORGANISMS, SOCIAL choice, Game Theory and Formal Modeling},
  Owner                    = {Bob},
  Timestamp                = {2014.07.14},
  Url                      = {http://www.proxy.its.virginia.edu/login?url=https://search.ebscohost.com/login.aspx?direct=true&db=bth&AN=7704421&site=ehost-live}
}

@Article{abadie2003,
  Title                    = {The Economic Costs of Conflict: A Case Study of the Basque Region},
  Author                   = {Alberto Abadie and Javier Gardeazabal},
  Year                     = {2003},

  Month                    = {March},
  Number                   = {1},
  Pages                    = {113-132},
  Volume                   = {93},

  Annotation               = {\newline They do a differences-in-differences estimation of terrorist attacks in the Basque region in Spain and the consequent effects on economic growth. They use weights to construct a ``synthetic" region from other Spanish districts as a control for the Basque region over time, finding a large difference in growth rates. They note that the common support condition is not perfect as the Basque region was more industrialized than any other region before the onset of fighting. Overall seems like a kind of propensity score matching without all the labels. \newline \newline What good is a franken unit for comparison? You can't write a narrative about it. Also, why not just use matching? What if the convex combination of units obscures a lot of differences in the units used in comparison? Why is your unit so unique you can't find a good control unit? Maybe you should explain that rather than using this algorithm. \newline},
  Journaltitle             = {The American Economic Review},
  Keywords                 = {Civil War, Differences-in-Differences, Matching},
  Owner                    = {Bob},
  Timestamp                = {2014.10.27}
}

