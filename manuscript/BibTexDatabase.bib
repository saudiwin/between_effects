@Book{King1994,
  Title                    = {Designing social inquiry: Scientific inference in qualitative research},
  Author                   = {King, Gary and Keohane, Robert O and Verba, Sidney},
  Publisher                = {Princeton University Press},
  Year                     = {1994},

  Annotation               = {\newline KKG think the most-similar/most-different designs are of limited utility, and instead advocate a statistical understanding of using "controls" in qualititative studies. They think that an explicit recognition of which variables are necessary controls, and then designing the research to incorporate them, is vastly superior to simply choosing most-similar or most-different. They do support a matching idea which is close to the most-similar design, except that they note that doing this can still leave omitted variables which may be even harder to detect because the cases are quite similar. To some extent, their talk of matching approaches that of a natural experiment, in which the difference between cases is assumed to be exogenous. \newline \newline Note that introducing a control can bias the study if you don't have enough cases to balance on. I.e., the story about prisoners--how do you match a person who was supposed to go to prison but didn't? If you can find that person, wouldn't that make them a very unusual case with other variables affecting the outcome? Note that this problem is worse with small-N than large-N. With large-N you have more room to say whether or not a case is anomalous. In other words, the counterfactual must be well-defined: units could have been in either treatment or control. For extreme units, this may not be plausible. \newline},
  File                     = {:C\:\\Users\\Bob\\Documents\\My Box Files\\PLAD 7090\\Readings\\KKV.ch.4-4.pdf:PDF},
