@Article{kuran1999availability,
  Title                    = {Availability cascades and risk regulation},
  Author                   = {Kuran, Timur and Sunstein, Cass R},
  Journal                  = {Stanford Law Review},
  Year                     = {1999},
  Pages                    = {683--768},

  Keywords                 = {Social Movement Theory},
  Owner                    = {Bob},
  Publisher                = {JSTOR},
  Review                   = {This has a great lit review on informational cascades and herd behavior. They note that cascades can also happen locally and note the importance of network cascades that happen over the internet – see W. BRIAN ARTHUR, INCREASING RETURNS AND PATH DEPENDENCE IN THE ECONOMY (1994); Paul A. David, The Hero and the Herd in Tech- nological History: Reflections on Thomas Edison and the Battle of the Systems, in FAVORITES OF FORTUNE: TECHNOLOGY, GROWTH, AND ECONOMIC DEVELOPMENT SINCE THE INDUSTRIAL REVOLUTION 72 (Patrice Higonnet, David S. Landes & Henry Rosovsky eds., 1991); and Thomas C. Schelling, Dynamic Models of Segregation, 1 J. MATHEMATICAL SOC. 143 (1971). Networking effects do not enter our analysis here. They say that informational cascades can be harmful if and when they are “availability” cascades organized around a single event that is improperly applied. They use the availability heuristic, although Kuran does not think that this makes someone “quasi-rational” (689-90). Rather, it is rational to use only part of your information given the lack of available information. Also, people should care (rationally) about their reputations. They look at case studies concerning instances where scientific evidence contradicted public opinion. This is a bit different than the availability heuristic of the Arab Spring. I.e., was it more rational for people in Tunisia to revolt? If so, why? Was it less rational to “play the lottery” in other countries? He talks about “rational ignorance”. Is the Arab Spring an example of rational ignorance? (704) This assumes that these social movements are successful. They argue from prospect theory that reference points can change people’s utility calculations. “Availability entrepreneurs” are successful when they are able to “induc[e] splits between the private and public ‘selves’ of a sufficient number of people” (727) – in other words, when enough people are willing to forsake their private judgment in favor of public judgment Two things cause information cascades: either caring about social approval or taking your informational cues from what society thinks is true (729) Information affects people’s private risk assessments, and reputational change affects people’s assessment of the costs of their public opinions (729-30) --so what happens when you have an “issue” that has enormously high costs, as in regime change? --as the cascade happens, people with opposing viewpoints are silenced, making the new consensus appear even more credible (730-31) Availability entrepreneurs often rely on symbolic events to start their movement (733) Are availability entrepreneurs mistrustful people who exploit the common consensus, or are they altruistic and committed to a cause (733-34) While sometimes the availability enterpreneurs are shading the truth, they may still result in a socially optimal outcome (735) --so would the Arab Spring be socially optimal? Were the actions of the entrepreneurs in the Arab Spring justified? Did they rely on preference falsification to “get out the vote”? They argue that public policy should not be based on public opinion because of preference falsification (737) Quote: “In and of themselves, people’s words and deeds concerning risks tell us too little about what they truly know, what they sincerely want, and what influences have shaped their dispositions, attitudes, and sensitivities” (740)},
  Timestamp                = {2014.01.21}
}

@Article{KYKL:KYKL3,
  Title                    = {Shared Mental Models: Ideologies and Institutions},
  Author                   = {Denzau, Arthur T. and North, Douglass C.},
  Journal                  = {Kyklos},
  Year                     = {1994},
  Number                   = {1},
  Pages                    = {3--31},
  Volume                   = {47},

  Annotation               = {\newline Denzau and North propose to provide a preliminary understanding of the limits of "substantive rationality". They propose that ideologies are created by individuals to deal with Knightian uncertainty. These are internal processes of the brain, but crucially, are made into "shared mental models" (SMMs) through communication between individuals. Individuals need to be able to communicate in order to coordinate; thus there is always a payoff to having a shared mental model versus an individual mental model. \newline They believe that Bayesian updating is a limited form of learning which does not capture truly paradigm-shifting knowledge changes, which they term "representational rediscription". A Bayesian update can never change the form of the distribution, only its parameters, thus it is limited. They propose that such rediscription only happens on rare occaisions when it seems that the ideology in vogue has become clearly logically false. Then, there is a quick adjustment to a new mental model in the form of a punctuated equilibrium. \newline They use Arrow's theorem and other conjectures to argue that ideologies are fundamentally complex and therefore can never be fully evaluated. Because of the potential logical inconsistency of any belief there is always the potential for it to change. Thus this change cannot be predicted, but it is almost certain that it will occur at some point due to logical inconsistencies in beliefs. They do not think that such inconsistencies disappear merely with a transition to a "modern" society.},
  Doi                      = {10.1111/j.1467-6435.1994.tb02246.x},
  ISSN                     = {1467-6435},
  Owner                    = {Robert Kubinec},
  Publisher                = {Blackwell Publishing Ltd},
  Timestamp                = {2014.06.30},
  Url                      = {http://dx.doi.org/10.1111/j.1467-6435.1994.tb02246.x}
}

@Article{laddlenz2009,
  Title                    = {Exploiting a Rare Communication Shift to Document the Persuasive Power of the News Media},
  Author                   = {Jonathan McDonald Ladd and Gabriel S. Lenz},
  Year                     = {2009},
  Number                   = {2},
  Pages                    = {394-410},
  Volume                   = {53},

  Annotation               = {\newline Ladd and Lenz look at the effect of newspaper endorsements on electoral outcomes in Britain. They argue that the shift of some conservative newspapers in favor of Labor's candidate had a strong effect at increasing votes for the Labor candidate over those voters who read newspapers that did not switch. To find the effect the use a British longitudinal study of election voters in which they are able to see what newspapers people read over time and how that correlates with later voting behavior. \newline \newline Treatment is clearly not randomly assigned. Newspaper readership is clearly endogenous. That is where matching is key. Good estimate for a token cause, but hard to generalize because it measures a single event, which is a conservative paper endorsing a liberal candidate. The usual ``endorsement treatment" is when a paper endorses a candidate in line with their general political stance. The rarity of the event is what allows them to find such a large, measurable effect. Note there is a problem with attrition--people may be opting out of the treatment group after the sun changed its editorial policy. This would over-estimate the level of voting. \newline \newline Endogeneity of treatment: is the newspaper anticipating the change of their voters and thus their editorial stance reflects underlying discontent? \newline},
  Journaltitle             = {American Journal of Political Science},
  Keywords                 = {Political Communication, Differences-in-Differences},
  Owner                    = {Bob},
  Timestamp                = {2014.10.27}
}

@Article{Lake2007,
  Title                    = {Escape from the State of Nature: Authority and Hierarchy in World Politics},
  Author                   = {Lake, David A.},
  Journal                  = {International Security},
  Year                     = {2007},

  Month                    = jun,
  Number                   = {1},
  Pages                    = {47--79},
  Volume                   = {32},

  Abstract                 = {Despite increasing attention, scholars lack the analytic tools necessary to understand international hierarchy and its consequences for politics and policy. This is especially true for the informal hierarchies now found in world affairs. Rooted in a formal-legal tradition, international relations scholars almost universally assume that the international system is a realm of anarchy. Although the fact of anarchy remains a truism for the system as a whole, it is a fallacy of division to infer that all relationships within that system are anarchic. Building on an alternative view of relational authority and recent research on the practice of sovereignty, a new conception of international hierarchy is developed that varies along two continua defined by security and economic relations. This construct is operationalized and validated, and then tested in a large-nstudy of the effects of international hierarchy on the defense effort of countries. The principal finding is that states in hierarchical relationships spend significantly less on defense relative to gross domestic product than states not in such relationships. In short, hierarchy matters and subordination pays; states appear to trade some portion of their sovereignty for protection from external security threats. Despite increasing attention, scholars lack the analytic tools necessary to understand international hierarchy and its consequences for politics and policy. This is especially true for the informal hierarchies now found in world affairs. Rooted in a formal-legal tradition, international relations scholars almost universally assume that the international system is a realm of anarchy. Although the fact of anarchy remains a truism for the system as a whole, it is a fallacy of division to infer that all relationships within that system are anarchic. Building on an alternative view of relational authority and recent research on the practice of sovereignty, a new conception of inte