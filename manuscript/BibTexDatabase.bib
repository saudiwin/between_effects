@Article{PPS:7804323,
  Title                    = {Ballots and Barricades: On the Reciprocal Relationship between Elections and Social Movements},
  Author                   = {McAdam,Doug and Tarrow,Sidney},
  Journal                  = {Perspectives on Politics},
  Year                     = {2010},

  Month                    = {6},
  Pages                    = {529--542},
  Volume                   = {8},

  Abstract                 = {ABSTRACT Why do two cognate literatures—social movements and electoral studies—travel along parallel paths with little conversation between them? And what can be done to connect them in the future? Drawing on their work with the late Charles Tilly on Dynamics of Contention (2001), Doug McAdam and Sidney Tarrow examine two important studies that approach (but do not effect such a linkage), propose a mechanism-based set of linkages between elections and social movements, and apply their approach in a preliminary examination of the relations between the American anti-war movement after 9/11 and the Democratic Party.},
  Annotation               = {--they are discussing the reaction to their book, Dynamics of Contention, which attempted to unite disparate fields in the study of CA, including civil wars, movements, and revolutions --they think game theory only shows "cognitive mechanism" not environmental or social network constraints (530-31) --they came up with the idea of "scale shift", or the changing of CA from one phase to another, such as passing through borders (531-2) --in this paper they want to examine what mechanisms link elections to social movements and vice versa: when do elections cause social movements to form, and when can social movements affect electoral outcomes (533) --movements can cause the consolidation of "electoral regimes" or periods of electoral dominance in the U.S. (such as a party controlling the president for a number of decades) (534) --in particular, they look at the "interprenetration" of movements of American political parties and vice versa (536) --they think parties tend towards coalitions (centrism) while movements tend towards "single issues": note this logic only holds for democracies/states not in transition (537) --they wonder if the internet will yield rapidly forming and rapidly dispersing social movements that have no staying power (538) --they want to look more at the connection between electoral politics and political violence (538) --they mention mechanisms, but it is not clear if these mechanisms exist or if they are proposed explanations for the U.S. political system. are they mechanisms or outcomes? I.e., do they specify the intermediate linkages/stages of the mechanism?},
  Doi                      = {10.1017/S1537592710001234},
  ISSN                     = {1541-0986},
  Issue                    = {02},
  Keywords                 = {Social Movement Theory, Nonviolent Social Movements, Revolutions},
  Numpages                 = {14},
  Owner                    = {Robert Kubinec},
  Timestamp                = {2014.05.27},
  Url                      = {http://journals.cambridge.org/article_S1537592710001234}
}

@Book{przeworski1970,
