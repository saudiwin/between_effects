@InBook{gilwhite2004,
  Title                    = {Ultimatum Game with an Ethnicity Manipulation},
  Author                   = {Francisco J. Gil-White},
  Editor                   = {Joseph Henrich and Robert Boyd and Samuel Bowles and Colin Camerer and Ernst Fehr and Herbert Gintis},
  Pages                    = {260-303},
  Publisher                = {Oxford University Press},
  Year                     = {2004},

  Annotation               = {\newline Gil-white presented his experiment performed in rural Mongolia. He went to a village that was inhabited by native Mongolians and also by nomadic Kazakhs. His first experiment went poorly after the game (Ultimatum game) was poorly understood by the players. In the second, he did a more sophisticated version in which he recruited members of both ethnicities, and while the game was anonymous, the players knew whether they were making an offer to a someone of their own ethnicity or someone else. The results of the experiment show that players tended to make higher offers to the out group as opposed to the in group, in contradiction to existing theory on in-group bias. Gil-white proposes this is because people of one ethnicity tend to know each other better and thus will make a lower offer as they have a better sense of where the refusal line is. On the other hand, people of other ethnicities are harder to predict, thus to gain any money at all one should make a higher offer. \newline \newline The troubles he had with the first experiment suggest that the field experiment did not work exactly as hoped, and it would be very hard to replicate. This questions some of the findings. He was able to talk to all the members of the experiment afterwards and investigate causal mechanisms, but some of his links between pastoralism as discouraging third-party punishment and the game are hard to substantiate without further fieldwork. Nonetheless, the piece does show the power of using even a simple game experiment in unusual settings. \newline},
  Booktitle                = {Foundations of Human Sociality: Economic Experiments and Ethnographic Evidence from Fifteen Small-scale Societies},
  Owner                    = {Bob},
  Timestamp                = {2014.09.09}
}

