@Article{munch2002,
  Title                    = {Conceptualizing and Measuring Democracy: Evaluating Alternative Indices},
  Author                   = {Gerardo L. Munck and Jay Verkuilen},
  Year                     = {2002},
  Number                   = {1},
  Pages                    = {5-34},
  Volume                   = {35},

  Annotation               = {\newline Munch and Verkuilen dig deep into existing democracy indices. They are highly critical of Freedom House and moderately critical of others, arguing that methodological decisions regarding democracy measurement have not reflected well-thought out trade-offs. \newline \newline They use a framework for measurement that involves defining the concept and all attributes of the concept, which involves a tree-leaf structure as the definition moves from the concept to lower-order attributes of the concept ("level of abstraction"). At the bottom level of abstraction, indicators should be chosen to reflect the abstraction of the concept at its lowest level. However, he notes that then the disaggregated attributes and their corresponding rules need to be aggregated together using a rule so that the index reflects the overall power of the concept. A lot of studies, if not most, do not use clearly defined aggregation rules. \newline \newline He wants researchers to avoid the dangers of "maximalist" definitions: "too many attributes" v. "minimalist" definitions: definition applies to too many cases. He notes that minimalist can also fail, as some, like Polity, include only contestation instead of participation, and others, like CGV, use a definition of "offices", or the number of offices which are actually elected. Thus it is hard to come up with truly minimalist definitions of democracy. \newline \newline Given the leaf-tree structure, there are two basic problems with measurement, and that is "conflation" (attributes lumped together without reason) and "redundancy" (identical attributes in different branches). Unlike others, he is not in favor of ratio scales, and instead argues that scales should have the minimum number of distinctions necessary to measure the concept adequately. \newline},
  Date                     = {February},
  Journaltitle             = {Comparative Political Studies},
  Keywords                 = {Democratic Transitions},
  Owner                    = {Robert Kubinec},
  Timestamp                = {2014.09.04}
}

