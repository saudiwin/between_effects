@Article{Carey01092000,
  Title                    = {Parchment, Equilibria, and Institutions},
  Author                   = {Carey, John M.},
  Journal                  = {Comparative Political Studies},
  Year                     = {2000},
  Number                   = {6-7},
  Pages                    = {735-761},
  Volume                   = {33},

  Abstract                 = {Institutions are rules that constrain political behavior. Although there is consensus that being written down is neither necessary nor sufficient for an institution to be effective, much research on comparative institutions focuses on formal, parchment, institutions. This article argues that parchment can contribute to the generation of shared mutual expectations among political actors, which are essential to the effectiveness of institutions. Next, the article distinguishes between research that emphasizes the role of institutions in aggregating preferences into political decisions and research that relies on coordination models to identify conditions favoring certain equilibrium outcomes when multiple equilibria are possible. The article notes the increasing prominence of such coordination models in research on comparative institutions and concludes with some reflections about the prospects for this trend to foster connections between institutional analysis and the field of comparative politics more broadly.},
  Annotation               = {\newline Carey argues that parchment rules do matter to later political outcomes because of the nature of coordination games, which generate multiple equilibria by nature. Given these multiple equilibria, it is possible that mutually-enforcing expectations can sustain outcomes even when many of the political actors are in disagreement over the imposition of the institution (esp. when these political actors face their own coordination problems). \newline Thus Carey proposes that coordination games resolve the dilemma of endogenous institutions. It is hard for actors to depart from one coordinating equilibrium to another, hence, that coordination equilibrium may last in spite of power imbalances and may perpetuate itself over the long run via path dependence. Carey wants scholars to focus more on the criteria that facilitate equilibrium selection and that generate self-reinforcing expectations producing these outcomes. \newline I like coordination games, but I do wonder if it is possible to focus so much on focal point selection as a mechanism. That is outside of the game and the game makes no predictions about how that happens. Reducing all of institutional theory to equilibriums selection seems a bit obtuse. Nonetheless, it does seem that one thing that sustains institutions over the long run is the tendency of actors to coordinate around them even when they don't like them.},
  Doi                      = {10.1177/001041400003300603},
  Eprint                   = {http://cps.sagepub.com/content/33/6-7/735.full.pdf+html},
  Keywords                 = {Institutional Theory},
  Owner                    = {Robert Kubinec},
  Timestamp                = {2014.06.30},
  Url                      = {http://cps.sagepub.com/content/33/6-7/735.abstract}
}

