@Article{sekhon2004,
  Title                    = {Quality Meets Quantity: Case Studies, Conditional Probability, and Counterfactuals},
  Author                   = {Jasjeet S. Sekhon},
  Year                     = {2004},

  Month                    = {June},
  Number                   = {2},
  Pages                    = {281-293},
  Volume                   = {2},

  Abstract                 = {\newline Sekhon argues that Geddes' critique of Skocpol is inaccurate if Skocpol is interpreted to be making probabilistic as opposed to deterministic statements about causation. That is to say, foreign intervention may raise the probability of revolution even if it does not always produce revolution. Thus he argues for the use of conditional probabilities as a way of understanding the outcomes of case studies. This is different from Mill's original methods, which assumed a deterministic aspect of causation devoid of measurement error. Conditional probalities are not synonymous with causation (they don't overcome the fundamental problem of causal inference), but they can provide a better framework for the conclusions reached by qualitative studies. It is a fundamentally Bayesian perspective: given our knowledge of the world, event X is more or less likely than expected. \newline},
  Annotation               = {\newline Deterministic statements are just implausible given the complexity of the world. Rather, we should use conditional probabilities as a way of understanding what qualitative inference is all about. Nothing is specific to qualitative research: just admit it is essentially quantitative with fewer observations. He wants to drop both the ``uniqueness" and ``determinism" assumptions of Przerworski and Teune. But if you have only 2 cases, how can you speculate about probabilities and possible outcomes? Note that he doesn't really care about mechanisms; inference can be done with and without mechanisms. As in aspirin--why did the guy win a nobel prize for finding the mechanism? You can figure out causation without mechanisms, but it is very incomplete. \newline \newline Sometimes, mechanisms may have normative implications, so it is still important to understand them. Also, if the outcome of interest isn't the only concern, then black-box causality isn't enough. \newline},
  Journaltitle             = {Perspectives on Politics},
  Keywords                 = {Causal Methodology, Qualitative},
  Owner                    = {Robert Kubinec},
  Timestamp                = {2014.09.25}
}

